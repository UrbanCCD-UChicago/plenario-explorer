<div class="row">
  <div class="col-md-5">
    {{filter-box startDate=_startDate endDate=_endDate agg=_agg res=resolution
        changedStartDate=(action "noOp")
        changedEndDate=(action "noOp")
        changedAgg=(action "noOp")
        resChanged=(action "resChanged")}}
    {{column-filters filters=mockFilters}}
    <br/>
      <button type="button" id="submit-query" class="btn btn-primary">
          <i class='fa fa-search'></i>
          Submit
      </button>
      <button type="button" id="reset-query" class="btn btn-default">
          <i class='fa fa-undo'></i>
          Reset
      </button>
  </div>
  <div class="col-md-7">
    {{heat-grid grid=model.grid datasetName=model.metadata.humanName}}
    {{high-charts classNames=chartClasses content=model.timeseries.series}}
  </div>
</div>
