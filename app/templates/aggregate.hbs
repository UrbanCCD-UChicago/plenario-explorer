{{aggregate-query-maker geoJSON=location_geom__within startDate=obs_date__ge endDate=obs_date__le agg=agg zoom=zoom
                        submit=(action "submit") reset=(action "reset")}}

<p class="lead"><strong>
  {{timeseriesList.length}} {{pluralize 'Event Dataset' timeseriesList.length}} Found
</strong></p>
{{point-aggregate-listing
  targetLinkRoute="event"
  displayLink=(action "navigateToPoint")
  downloadLink=(action "downloadPoint")
  pointDatasets=timeseriesList}}
<br/>
<p class="lead"><strong>
  {{model.shapeDatasets.length}} {{pluralize 'Shape Dataset' model.shapeDatasets.length}} Found
</strong></p>
{{shape-listing
  targetLinkRoute="shape"
  displayLink=(action "navigateToShape")
  downloadLink=(action "downloadShape")
  shapeDatasets=model.shapeDatasets}}

{{outlet}}
