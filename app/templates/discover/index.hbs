<div class="row">
    <div class="col-md-12">
      <h3><strong>{{model.pointDatasets.length}}</strong>
      Event Datasets Available to Query</h3>
      {{point-index-listing
      displayLink=(action "navigateToPoint")
      pointDatasets=processedPointDatasets}}
    </div>
</div>
<div class="row">
  <div class="col-md-12">
    <h3><strong>{{model.shapeDatasets.length}}</strong>
        Shape Datasets Available to Query</h3>

    {{shape-listing
    displayLink=(action "navigateToShape")
    downloadLink=(action "downloadShape")
    shapeDatasets=model.shapeDatasets}}
  </div>
</div>